{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "node scripts/generate-favicons.js && vite build",
    "build:no-icons": "vite build",
    "generate-favicons": "node scripts/generate-favicons.js",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage --run",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run test && npm run test:e2e",
    "test:ci": "npm run test:coverage && npm run test:e2e",
    "playwright:install": "playwright install",
    "playwright:install-deps": "playwright install-deps",
    "css:analyze": "node scripts/css-analyzer.cjs analyze",
    "css:consolidate": "node scripts/css-analyzer.cjs consolidate",
    "css:clean": "node scripts/css-analyzer.cjs clean",
    "build:demo": "chmod +x scripts/build-demo.sh && ./scripts/build-demo.sh"
  },
  "dependencies": {
    "lucide-react": "^0.564.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "reactflow": "^11.11.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.52.0",
    "@tailwindcss/postcss": "^4.1.7",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^4.0.0",
    "@vitest/ui": "^4.0.0",
    "jsdom": "^28.0.0",
    "vite": "^7.0.0",
    "vitest": "^4.0.0"
  }
}
